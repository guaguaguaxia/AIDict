{
  "git": {
    "deploymentEnabled": false
  },
  "buildCommand": "cd aidict-web && npm install && npm run build",
  "outputDirectory": "aidict-web/.next",
  "framework": "nextjs",
  "rewrites": [
    { "source": "/(.*)", "destination": "/aidict-web/$1" }
  ],
  "github": {
    "silent": true
  }
}